      <instance>
        <scdsc_investigation delimiter="#" id="scdsc_investigation" prefix="J1!scdsc_investigation!" version="2019-09-05">
          <inputs>
            <meta>
              <location>
                <lat/>
                <long/>
                <error/>
                <message/>
              </location>
            </meta>
            <id_signal/>
          </inputs>
          <patient_id/>
          <signal_id/>
          <scdsc_investigation>
            <scdc_informed_date/>
            <investigation_start_date/>
            <event_start_date/>
            <signs_symptoms/>
            <cause_known/>
            <cause/>
            <lab_results_known/>
            <collected_lab_samples/>
            <no_people_exposed/>
            <no_human_cases/>
            <no_human_cases_hospitalized/>
            <no_human_deaths/>
            <no_animals_exposed/>
            <no_sick_animals/>
            <no_dead_animals/>
            <new_cases_initial_area/>
            <new_cases_new_areas/>
            <setting_promotes_spread/>
            <additional_info/>
            <group_event_measures>
              <event_measures/>
              <human_attack_rate/>
              <n_human_attack_rate/>
              <human_hospitalization_rate/>
              <n_human_hospitalization_rate/>
              <human_case_fatality_rate/>
              <n_human_case_fatality_rate/>
              <animal_attack_rate/>
              <n_animal_attack_rate/>
              <animal_case_fatality_rate/>
              <n_animal_case_fatality_rate/>
            </group_event_measures>
            <event_risk_classificaiton/>
            <response_recommedations/>
            <response_start_date/>
          </scdsc_investigation>
          <meta tag="hidden">
            <instanceID/>
          </meta>
        </scdsc_investigation>
      </instance>
      <instance id="contact-summary"/>
      <bind nodeset="/scdsc_investigation/inputs" relevant="0"/>
      <bind nodeset="/scdsc_investigation/inputs/id_signal" type="string"/>
      <bind calculate=" /scdsc_investigation/inputs/id_signal " nodeset="/scdsc_investigation/patient_id" type="string"/>
      <bind calculate=" /scdsc_investigation/inputs/id_signal " constraint="regex(.,'^[0-9]{5,13}$')" jr:constraintMsg="The signal ID should be a 5 to 13 digit number. Kindly enter a valid signal ID." nodeset="/scdsc_investigation/signal_id" required="true()" type="string"/>
      <bind jr:constraintMsg="Kindly select a date that is not be in the future and not more than 3 months ago." nodeset="/scdsc_investigation/scdsc_investigation/scdc_informed_date" required="true()" type="date"/>
      <bind jr:constraintMsg="Kindly select a date that is not be in the future and not more than 3 months ago." nodeset="/scdsc_investigation/scdsc_investigation/investigation_start_date" required="true()" type="date"/>
      <bind jr:constraintMsg="Kindly select a date that is not be in the future and not more than 3 months ago." nodeset="/scdsc_investigation/scdsc_investigation/event_start_date" required="true()" type="dateTime"/>
      <bind nodeset="/scdsc_investigation/scdsc_investigation/signs_symptoms" required="true()" type="string"/>
      <bind nodeset="/scdsc_investigation/scdsc_investigation/cause_known" required="true()" type="select1"/>
      <bind nodeset="/scdsc_investigation/scdsc_investigation/cause" relevant=" /scdsc_investigation/scdsc_investigation/cause_known  = 'yes'" required="true()" type="string"/>
      <bind nodeset="/scdsc_investigation/scdsc_investigation/lab_results_known" relevant=" /scdsc_investigation/scdsc_investigation/cause_known  = 'yes'" required="true()" type="select1"/>
      <bind nodeset="/scdsc_investigation/scdsc_investigation/collected_lab_samples" relevant=" /scdsc_investigation/scdsc_investigation/cause_known  = 'no' or  /scdsc_investigation/scdsc_investigation/lab_results_known  = 'no'" required="true()" type="select1"/>
      <bind nodeset="/scdsc_investigation/scdsc_investigation/no_people_exposed" required="true()" type="int"/>
      <bind constraint=". &lt;=  /scdsc_investigation/scdsc_investigation/no_people_exposed " jr:constraintMsg="Number of human cases cannot be more than number of people exposed." nodeset="/scdsc_investigation/scdsc_investigation/no_human_cases" required="true()" type="int"/>
      <bind constraint=". &lt;=  /scdsc_investigation/scdsc_investigation/no_human_cases " jr:constraintMsg="Number of human cases hospitalized cannot be more than number of human cases." nodeset="/scdsc_investigation/scdsc_investigation/no_human_cases_hospitalized" required="true()" type="int"/>
      <bind constraint=". &lt;=  /scdsc_investigation/scdsc_investigation/no_human_cases " jr:constraintMsg="Number of human deaths cannot be more than number of human cases." nodeset="/scdsc_investigation/scdsc_investigation/no_human_deaths" required="true()" type="int"/>
      <bind nodeset="/scdsc_investigation/scdsc_investigation/no_animals_exposed" required="true()" type="int"/>
      <bind constraint=". &lt;=  /scdsc_investigation/scdsc_investigation/no_animals_exposed " jr:constraintMsg="Number of sick animals cannot be more than number of animals exposed." nodeset="/scdsc_investigation/scdsc_investigation/no_sick_animals" required="true()" type="int"/>
      <bind constraint=". &lt;=  /scdsc_investigation/scdsc_investigation/no_sick_animals " jr:constraintMsg="Number of dead animals cannot be more than number of sick animals." nodeset="/scdsc_investigation/scdsc_investigation/no_dead_animals" required="true()" type="int"/>
      <bind nodeset="/scdsc_investigation/scdsc_investigation/new_cases_initial_area" required="true()" type="select1"/>
      <bind nodeset="/scdsc_investigation/scdsc_investigation/new_cases_new_areas" required="true()" type="select1"/>
      <bind nodeset="/scdsc_investigation/scdsc_investigation/setting_promotes_spread" required="true()" type="select1"/>
      <bind nodeset="/scdsc_investigation/scdsc_investigation/additional_info" required="false()" type="string"/>
      <bind nodeset="/scdsc_investigation/scdsc_investigation/group_event_measures/event_measures" readonly="true()" type="string"/>
      <bind calculate="if( /scdsc_investigation/scdsc_investigation/no_human_cases  = 0 or  /scdsc_investigation/scdsc_investigation/no_people_exposed  = 0, 0, round(( /scdsc_investigation/scdsc_investigation/no_human_cases  div  /scdsc_investigation/scdsc_investigation/no_people_exposed ) * 100, 2))" nodeset="/scdsc_investigation/scdsc_investigation/group_event_measures/human_attack_rate" relevant=" /scdsc_investigation/scdsc_investigation/no_people_exposed  &gt; 0" type="string"/>
      <bind nodeset="/scdsc_investigation/scdsc_investigation/group_event_measures/n_human_attack_rate" readonly="true()" relevant=" /scdsc_investigation/scdsc_investigation/no_people_exposed  &gt; 0" type="string"/>
      <bind calculate="if( /scdsc_investigation/scdsc_investigation/no_human_cases_hospitalized  = 0 or  /scdsc_investigation/scdsc_investigation/no_human_cases  = 0, 0, round(( /scdsc_investigation/scdsc_investigation/no_human_cases_hospitalized  div  /scdsc_investigation/scdsc_investigation/no_human_cases ) * 100, 2))" nodeset="/scdsc_investigation/scdsc_investigation/group_event_measures/human_hospitalization_rate" relevant=" /scdsc_investigation/scdsc_investigation/no_people_exposed  &gt; 0" type="string"/>
      <bind nodeset="/scdsc_investigation/scdsc_investigation/group_event_measures/n_human_hospitalization_rate" readonly="true()" relevant=" /scdsc_investigation/scdsc_investigation/no_people_exposed  &gt; 0" type="string"/>
      <bind calculate="if( /scdsc_investigation/scdsc_investigation/no_human_deaths  = 0 or  /scdsc_investigation/scdsc_investigation/no_human_cases  = 0, 0, round(( /scdsc_investigation/scdsc_investigation/no_human_deaths  div  /scdsc_investigation/scdsc_investigation/no_human_cases ) * 100, 2))" nodeset="/scdsc_investigation/scdsc_investigation/group_event_measures/human_case_fatality_rate" relevant=" /scdsc_investigation/scdsc_investigation/no_people_exposed  &gt; 0" type="string"/>
      <bind nodeset="/scdsc_investigation/scdsc_investigation/group_event_measures/n_human_case_fatality_rate" readonly="true()" relevant=" /scdsc_investigation/scdsc_investigation/no_people_exposed  &gt; 0" type="string"/>
      <bind calculate="if( /scdsc_investigation/scdsc_investigation/no_sick_animals  = 0 or  /scdsc_investigation/scdsc_investigation/no_animals_exposed  = 0, 0, round(( /scdsc_investigation/scdsc_investigation/no_sick_animals  div  /scdsc_investigation/scdsc_investigation/no_animals_exposed ) * 100, 2))" nodeset="/scdsc_investigation/scdsc_investigation/group_event_measures/animal_attack_rate" relevant=" /scdsc_investigation/scdsc_investigation/no_animals_exposed  &gt; 0" type="string"/>
      <bind nodeset="/scdsc_investigation/scdsc_investigation/group_event_measures/n_animal_attack_rate" readonly="true()" relevant=" /scdsc_investigation/scdsc_investigation/no_animals_exposed  &gt; 0" type="string"/>
      <bind calculate="if( /scdsc_investigation/scdsc_investigation/no_dead_animals  = 0 or  /scdsc_investigation/scdsc_investigation/no_sick_animals  = 0, 0, round(( /scdsc_investigation/scdsc_investigation/no_dead_animals  div  /scdsc_investigation/scdsc_investigation/no_sick_animals ) * 100, 2))" nodeset="/scdsc_investigation/scdsc_investigation/group_event_measures/animal_case_fatality_rate" relevant=" /scdsc_investigation/scdsc_investigation/no_animals_exposed  &gt; 0" type="string"/>
      <bind nodeset="/scdsc_investigation/scdsc_investigation/group_event_measures/n_animal_case_fatality_rate" readonly="true()" relevant=" /scdsc_investigation/scdsc_investigation/no_animals_exposed  &gt; 0" type="string"/>
      <bind nodeset="/scdsc_investigation/scdsc_investigation/event_risk_classificaiton" required="true()" type="select1"/>
      <bind nodeset="/scdsc_investigation/scdsc_investigation/response_recommedations" required="true()" type="select"/>
      <bind jr:constraintMsg="Kindly select a date that is not be in the future and not more than 3 months ago." nodeset="/scdsc_investigation/scdsc_investigation/response_start_date" required="true()" type="date"/>
      <bind calculate="concat('uuid:', uuid())" nodeset="/scdsc_investigation/meta/instanceID" readonly="true()" type="string"/>
    </model>
  </h:head>
  <h:body class="pages">
    <group appearance="field-list" ref="/scdsc_investigation/inputs">
      <input appearance="hidden" ref="/scdsc_investigation/inputs/id_signal"/>
    </group>
    <group appearance="field-list" ref="/scdsc_investigation/scdsc_investigation">
      <label ref="jr:itext('/scdsc_investigation/scdsc_investigation:label')"/>
      <input appearance="no-calendar" ref="/scdsc_investigation/scdsc_investigation/scdc_informed_date">
        <label ref="jr:itext('/scdsc_investigation/scdsc_investigation/scdc_informed_date:label')"/>
      </input>
      <input appearance="no-calendar" ref="/scdsc_investigation/scdsc_investigation/investigation_start_date">
        <label ref="jr:itext('/scdsc_investigation/scdsc_investigation/investigation_start_date:label')"/>
      </input>
      <input appearance="no-calendar" ref="/scdsc_investigation/scdsc_investigation/event_start_date">
        <label ref="jr:itext('/scdsc_investigation/scdsc_investigation/event_start_date:label')"/>
      </input>
      <input ref="/scdsc_investigation/scdsc_investigation/signs_symptoms">
        <label ref="jr:itext('/scdsc_investigation/scdsc_investigation/signs_symptoms:label')"/>
      </input>
      <select1 ref="/scdsc_investigation/scdsc_investigation/cause_known">
        <label ref="jr:itext('/scdsc_investigation/scdsc_investigation/cause_known:label')"/>
        <item>
          <label>Yes</label>
          <value>yes</value>
        </item>
        <item>
          <label>No</label>
          <value>no</value>
        </item>
      </select1>
      <input ref="/scdsc_investigation/scdsc_investigation/cause">
        <label ref="jr:itext('/scdsc_investigation/scdsc_investigation/cause:label')"/>
      </input>
      <select1 ref="/scdsc_investigation/scdsc_investigation/lab_results_known">
        <label ref="jr:itext('/scdsc_investigation/scdsc_investigation/lab_results_known:label')"/>
        <item>
          <label>Yes</label>
          <value>yes</value>
        </item>
        <item>
          <label>No</label>
          <value>no</value>
        </item>
      </select1>
      <select1 ref="/scdsc_investigation/scdsc_investigation/collected_lab_samples">
        <label ref="jr:itext('/scdsc_investigation/scdsc_investigation/collected_lab_samples:label')"/>
        <item>
          <label>Yes</label>
          <value>yes</value>
        </item>
        <item>
          <label>No</label>
          <value>no</value>
        </item>
        <item>
          <label>Not applicable</label>
          <value>not_applicable</value>
        </item>
      </select1>
      <input ref="/scdsc_investigation/scdsc_investigation/no_people_exposed">
        <label ref="jr:itext('/scdsc_investigation/scdsc_investigation/no_people_exposed:label')"/>
      </input>
      <input ref="/scdsc_investigation/scdsc_investigation/no_human_cases">
        <label ref="jr:itext('/scdsc_investigation/scdsc_investigation/no_human_cases:label')"/>
      </input>
      <input ref="/scdsc_investigation/scdsc_investigation/no_human_cases_hospitalized">
        <label ref="jr:itext('/scdsc_investigation/scdsc_investigation/no_human_cases_hospitalized:label')"/>
      </input>
